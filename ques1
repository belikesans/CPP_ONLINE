#include <iostream>
using namespace std;
int main() {
    std::string str1, str2;

    std::cout << "Enter the first string: ";
    std::cin >> str1;  // Use cin for basic input (no spaces)

    std::cout << "Enter the second string: ";
    std::cin >> str2;

    std::string concatenatedString = str1 + str2;

    std::cout << "The concatenated string is: " << concatenatedString << std::endl;

    return 0;
}
